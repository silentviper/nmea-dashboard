{
	"name": "nmea-dashboard",
	"version": "0.0.1",
	"private": true,
	"signalk": {
		"appIcon": "./img/icon-72x72.png",
		"displayName": "NMEA Dashboard"
	},
	"keywords": [
		"signalk-webapp",
		"signalk-node-server-plugin",
		"instruments",
		"dashboard",
		"marine"
	],
	"signalk-plugin-enabled-by-default": true,
	"main": "index.js",
	"scripts": {
		"postinstall": "node postinstall.js",
		"build": "cd vue-app && npm install && npm run build",
		"dev": "cd vue-app && npm run dev",
		"lint": "cd vue-app && npm run lint",
		"type-check": "cd vue-app && npm run type-check"
	}
}
